        { kbd: ' +  + L', text: lang.paragraph.left },
        { kbd: ' +  + E', text: lang.paragraph.center },
        { kbd: ' +  + R', text: lang.paragraph.right },
        { kbd: ' +  + J', text: lang.paragraph.justify },
        { kbd: ' +  + NUM7', text: lang.lists.ordered },
        { kbd: ' +  + NUM8', text: lang.lists.unordered }
