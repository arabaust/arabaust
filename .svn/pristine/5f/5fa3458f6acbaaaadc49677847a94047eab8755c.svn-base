        { kbd: ' + NUM1', text: lang.style.h1 },
        { kbd: ' + NUM2', text: lang.style.h2 },
        { kbd: ' + NUM3', text: lang.style.h3 },
        { kbd: ' + NUM4', text: lang.style.h4 },
