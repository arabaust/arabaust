<?php

namespace Admailer\Models;

use Illuminate\Database\Eloquent\Model;

class Media extends Model
{
    protected $table = 'ad_media';

    /**
     * The attributes that are mass assignable.
     *
     * @var array
     */
    protected $fillable = [
        'ad_id',
        'file_type',
        'filename',
        'description',
        'code',
        'title',
        'duration',
        'first_transmission_date',
        'last_transmission_date',
        'voice_over',
        'studio',
        'recording_date',
        'producer',
        'copywriter',
        'music'
    ];
}
